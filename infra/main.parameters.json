{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "appServicePlanName": {
      "value": "${AZURE_APP_SERVICE_PLAN}"
    },
    "cosmosDbAccountName": {
      "value": "${AZURE_COSMOSDB_ACCOUNT}"
    },
    "cosmosdbName": {
      "value": "${AZURE_COSMOSDB_DATABASE}"
    },
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "functionAppName": {
      "value":  "${AZURE_FUNCTION_APP}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "logAnalyticsName": {
      "value": "${AZURE_LOG_ANALYTICS}"
    },
    "principalId": {
      "value": "${AZURE_PRINCIPAL_ID}"
    },
    "publicNetworkAccess": {
      "value": "${AZURE_PUBLIC_NETWORK_ACCESS=Enabled}"
    },
    "storageAccountName": {
      "value": "${AZURE_STORAGE_ACCOUNT}"
    },
    "storageSkuName": {
      "value": "${AZURE_STORAGE_SKU=Standard_LRS}"
    },
    "useApplicationInsights": {
      "value": "${AZURE_USE_APPLICATION_INSIGHTS=true}"
    }
  }
}
